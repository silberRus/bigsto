// Для подсистемы "Варианты отчетов" при работе в модели сервиса.
Функция ВариантыНастроек() Экспорт
	
	Результат = Новый Массив;
	
	Вариант = Новый Структура;
	Вариант.Вставить("Имя",           "ФинансовоеСостояниеБазовая");
	Вариант.Вставить("Представление", НСтр("ru = 'Финансовое состояние'"));
	Результат.Добавить(Вариант);
	
	Возврат Результат;
	
