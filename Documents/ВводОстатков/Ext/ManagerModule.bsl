// Регистрирует данные для обработчика обновления ОбработатьДанныеДляПереходаНаНовуюВерсию
//
Процедура ЗарегистрироватьДанныеКОбработкеДляПереходаНаНовуюВерсию(Параметры) Экспорт
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ВводОстатков.Ссылка КАК Ссылка
	|ИЗ
	|	Документ.ВводОстатков КАК ВводОстатков
	|ГДЕ
	|	ВводОстатков.УдалитьРазделУчета <> ЗНАЧЕНИЕ(Перечисление.УдалитьРазделыВводаНачальныхОстатков.ПустаяСсылка)
	|
	|ОБЪЕДИНИТЬ
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ВводОстатков.Ссылка
	|ИЗ
	|	Документ.ВводОстатков.Товары КАК ВводОстатков
	|ГДЕ
	|	ВводОстатков.ИдентификаторСтроки = """"
	|
	|ОБЪЕДИНИТЬ
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ВводОстатков.Ссылка
	|ИЗ
	|	Документ.ВводОстатков КАК ВводОстатков
	|ГДЕ
	|	ВводОстатков.ТипОперации В (
	|		ЗНАЧЕНИЕ(Перечисление.ТипыОперацийВводаОстатков.ОстаткиСобственныхТоваров),
	|		ЗНАЧЕНИЕ(Перечисление.ТипыОперацийВводаОстатков.ОстаткиВозвратнойТарыПринятойОтПоставщиков),
	|		ЗНАЧЕНИЕ(Перечисление.ТипыОперацийВводаОстатков.ОстаткиМатериаловПереданныхВПроизводство),
	|		ЗНАЧЕНИЕ(Перечисление.ТипыОперацийВводаОстатков.ОстаткиМатериаловПереданныхПереработчикам),
	|		ЗНАЧЕНИЕ(Перечисление.ТипыОперацийВводаОстатков.ОстаткиПрочихРасходов))
	|	И ВводОстатков.НалогообложениеНДС <> ЗНАЧЕНИЕ(Перечисление.ТипыНалогообложенияНДС.ПустаяСсылка)
	|	И ВводОстатков.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ТипыНалогообложенияНДС.ПустаяСсылка)
	|";
	
	ОбновлениеИнформационнойБазы.ОтметитьКОбработке(Параметры, Запрос.Выполнить().Выгрузить().ВыгрузитьКолонку("Ссылка"));
	
КонецПроцедуры

// Обработчик обновления УТ 11.3.1:
// - переносит данные о назначении из вида запасов в аналитику учета номенклатуры;
// - заполняет новый реквизит РазделУчета значением по умолчанию.
//
Процедура ОбработатьДанныеДляПереходаНаНовуюВерсию(Параметры) Экспорт
	
	ПолноеИмяОбъекта = "Документ.ВводОстатков";
	
	ОбъектыДляОбработки = ОбновлениеИнформационнойБазы.ВыбратьСсылкиДляОбработки(Параметры.Очередь, ПолноеИмяОбъекта);
	
	Пока ОбъектыДляОбработки.Следующий() Цикл
		
		НачатьТранзакцию();
		
		Попытка
			
			// Устанавливаем управляемую блокировку, чтобы провести ответственное чтение объекта
			Блокировка = Новый БлокировкаДанных;
			
			ЭлементБлокировки = Блокировка.Добавить(ПолноеИмяОбъекта);
			ЭлементБлокировки.УстановитьЗначение("Ссылка", ОбъектыДляОбработки.Ссылка);
			
			Блокировка.Заблокировать();
			
			Объект = ОбъектыДляОбработки.Ссылка.ПолучитьОбъект();
			
			Если Объект = Неопределено Тогда
				ОтменитьТранзакцию();
				ОбновлениеИнформационнойБазы.ОтметитьВыполнениеОбработки(ОбъектыДляОбработки.Ссылка);
				Продолжить;
			КонецЕсли;
			
			#Область ЗаполнениеСуммУпр
			
			// Заполним новые реквизиты СуммаУпр пересчетом исходной суммы по курсу (как это делается при проведении)
			ТабличныеЧастиДляПересчета = Новый Массив;
			СуммыДляПересчета = Новый Структура;
			ВалютыДляПересчета = Новый Структура;
			
			ТабличныеЧастиДляПересчета.Добавить("Кассы");
			СуммыДляПересчета.Вставить("Кассы", "Сумма");
			ВалютыДляПересчета.Вставить("Кассы", "Касса.ВалютаДенежныхСредств");
			
			ТабличныеЧастиДляПересчета.Добавить("КассыККМ");
			СуммыДляПересчета.Вставить("КассыККМ", "Сумма");
			ВалютыДляПересчета.Вставить("КассыККМ", "КассаККМ.ВалютаДенежныхСредств");
			
			ТабличныеЧастиДляПересчета.Добавить("БанковскиеСчета");
			СуммыДляПересчета.Вставить("БанковскиеСчета", "Сумма");
			ВалютыДляПересчета.Вставить("БанковскиеСчета", "БанковскийСчет.ВалютаДенежныхСредств");
			
			ТабличныеЧастиДляПересчета.Добавить("РасчетыПоЭквайрингу");
			СуммыДляПересчета.Вставить("РасчетыПоЭквайрингу", "Сумма");
			ВалютыДляПересчета.Вставить("РасчетыПоЭквайрингу", "ЭквайринговыйТерминал.БанковскийСчет.ВалютаДенежныхСредств");
			
			ТабличныеЧастиДляПересчета.Добавить("РасчетыСПодотчетниками");
			СуммыДляПересчета.Вставить("РасчетыСПодотчетниками", "Сумма");
			ВалютыДляПересчета.Вставить("РасчетыСПодотчетниками", "Валюта");
			
			ТабличныеЧастиДляПересчета.Добавить("РасчетыПоДоговорамКредитовИДепозитов");
			СуммыДляПересчета.Вставить("РасчетыПоДоговорамКредитовИДепозитов", "Сумма");
			ВалютыДляПересчета.Вставить("РасчетыПоДоговорамКредитовИДепозитов", "Договор.ВалютаВзаиморасчетов");
			
			ТабличныеЧастиДляПересчета.Добавить("РасчетыМеждуОрганизациями");
			СуммыДляПересчета.Вставить("РасчетыМеждуОрганизациями", "Сумма");
			ВалютыДляПересчета.Вставить("РасчетыМеждуОрганизациями", "ВалютаВзаиморасчетов");
			
			ТабличныеЧастиДляПересчета.Добавить("РасчетыСПартнерами");
			СуммыДляПересчета.Вставить("РасчетыСПартнерами", "Сумма");
			ВалютыДляПересчета.Вставить("РасчетыСПартнерами", "ВалютаВзаиморасчетов");
			
			ТабличныеЧастиДляПересчета.Добавить("ПодарочныеСертификаты");
			СуммыДляПересчета.Вставить("ПодарочныеСертификаты", "СуммаВВалютеСертификата");
			ВалютыДляПересчета.Вставить("ПодарочныеСертификаты", "ПодарочныйСертификат.Владелец.Валюта");
			
			ВалютаУпрУчета = Константы.ВалютаУправленческогоУчета.Получить();
			КэшКоэффициентовПересчета = Новый Соответствие;
			
			Для каждого ТЧ Из ТабличныеЧастиДляПересчета Цикл
				
				Для каждого Строка Из Объект[ТЧ] Цикл
					
					Если Строка.СуммаУпр <> 0 Тогда
						Продолжить;
					КонецЕсли;
					
					ИсточникВалюты = ВалютыДляПересчета[ТЧ];
					МассивПодстрок = СтрРазделить(ИсточникВалюты, ".");
					ОбъектИсточник = МассивПодстрок[0];
					Если МассивПодстрок.Количество() = 1 Тогда
						Валюта = Строка[ОбъектИсточник];
					Иначе
						МассивРеквизит = ОбщегоНазначенияКлиентСервер.СкопироватьМассив(МассивПодстрок);
						МассивРеквизит.Удалить(0);
						Реквизит = СтрСоединить(МассивРеквизит, ".");
						Валюта = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Строка[ОбъектИсточник], Реквизит);
					КонецЕсли;
					
					Коэффициент =  КэшКоэффициентовПересчета[Валюта];
					Если Коэффициент = Неопределено Тогда
						Коэффициент = РаботаСКурсамиВалютУТ.ПолучитьКоэффициентПересчетаИзВалютыВВалюту(
										Валюта, ВалютаУпрУчета, Объект.Дата); 
						КэшКоэффициентовПересчета.Вставить(Валюта, Коэффициент);
					КонецЕсли;
					
					ИсточникСуммы = СуммыДляПересчета[ТЧ];
					Строка.СуммаУпр = Строка[ИсточникСуммы] * Коэффициент;
					
				КонецЦикла;
				
			КонецЦикла;
			
			#КонецОбласти
			
			#Область ОчисткаРазделаУчета
			// Очистим раздел учета
			
			Если Объект.УдалитьРазделУчета = Перечисления.УдалитьРазделыВводаНачальныхОстатков.ОперативныйИРегламентированныйУчет
				ИЛИ Объект.УдалитьРазделУчета = Перечисления.УдалитьРазделыВводаНачальныхОстатков.ОперативныйУчет Тогда
				Объект.ОтражатьВОперативномУчете = Истина;
			КонецЕсли;
			
			Если Объект.УдалитьРазделУчета = Перечисления.УдалитьРазделыВводаНачальныхОстатков.ОперативныйИРегламентированныйУчет
				ИЛИ Объект.УдалитьРазделУчета = Перечисления.УдалитьРазделыВводаНачальныхОстатков.РегламентированныйУчет Тогда
				Объект.ОтражатьВБУиНУ = Истина;
			КонецЕсли;
			
			Объект.УдалитьРазделУчета = Неопределено;
			
			#КонецОбласти
			
			#Область ОбработкаТЧТовары
			// Заполним идентификатор строки в ТЧ Товары
			Для каждого Строка Из Объект.Товары Цикл
				Если Не ЗначениеЗаполнено(Строка.ИдентификаторСтроки) Тогда
					Строка.ИдентификаторСтроки = Строка(Новый УникальныйИдентификатор);
				КонецЕсли;
			КонецЦикла;
			
			#КонецОбласти
			
			#Область ЗаполнениеВидаДеятельностиНДС
			Если (Объект.ТипОперации = Перечисления.ТипыОперацийВводаОстатков.ОстаткиСобственныхТоваров
						ИЛИ Объект.ТипОперации = Перечисления.ТипыОперацийВводаОстатков.ОстаткиВозвратнойТарыПринятойОтПоставщиков
						ИЛИ Объект.ТипОперации = Перечисления.ТипыОперацийВводаОстатков.ОстаткиМатериаловПереданныхВПроизводство
						ИЛИ Объект.ТипОперации = Перечисления.ТипыОперацийВводаОстатков.ОстаткиМатериаловПереданныхПереработчикам
						ИЛИ Объект.ТипОперации = Перечисления.ТипыОперацийВводаОстатков.ОстаткиПрочихРасходов)
					И Объект.НалогообложениеНДС <> Перечисления.ТипыНалогообложенияНДС.ПустаяСсылка()
					И Объект.ВидДеятельностиНДС = Перечисления.ТипыНалогообложенияНДС.ПустаяСсылка() Тогда
				Объект.ВидДеятельностиНДС = Объект.НалогообложениеНДС;
				Объект.НалогообложениеНДС = Перечисления.ТипыНалогообложенияНДС.ПустаяСсылка();
			КонецЕсли;
			#КонецОбласти
			
			ОбновлениеИнформационнойБазы.ЗаписатьДанные(Объект);
			
			ЗафиксироватьТранзакцию();
			
		Исключение
			
			ОтменитьТранзакцию();
			
			МетаданныеОбъекта = Метаданные.НайтиПоПолномуИмени(ПолноеИмяОбъекта);
			
			ТекстСообщения = НСтр("ru = 'Не удалось обработать %ИмяОбъекта%: %Ссылка% по причине: %Причина%'");
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%ИмяОбъекта%", ПолноеИмяОбъекта);
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%Ссылка%",     ОбъектыДляОбработки.Ссылка);
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%Причина%",    ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			
			ЗаписьЖурналаРегистрации(ОбновлениеИнформационнойБазы.СобытиеЖурналаРегистрации(),
										УровеньЖурналаРегистрации.Предупреждение,
										МетаданныеОбъекта,
										ОбъектыДляОбработки.Ссылка,
										ТекстСообщения);
			
		КонецПопытки;
		
	КонецЦикла;
	
	Параметры.ОбработкаЗавершена = ОбновлениеИнформационнойБазы.ОбработкаДанныхЗавершена(Параметры.Очередь, ПолноеИмяОбъекта);
	
КонецПроцедуры

#КонецОбласти

#КонецОбласти

#КонецЕсли
