			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	НЕОПРЕДЕЛЕНО КАК Ссылка,
	ТИПЗНАЧЕНИЯ(ЗНАЧЕНИЕ(Документ.ВнутреннееПотреблениеТоваров.ПустаяСсылка)) КАК ТипРаспоряжения,
	ДАТАВРЕМЯ(1, 1, 1) КАК Дата,
	ВЫРАЗИТЬ("" КАК СТРОКА(11)) КАК Номер,
	ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка) КАК Подразделение,
	ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка) КАК Организация,
	ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка) КАК Склад,
	ЗНАЧЕНИЕ(Перечисление.СтатусыВнутреннихЗаказов.ПустаяСсылка) КАК Статус,
	ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка) КАК Ответственный,
	ВЫРАЗИТЬ("" КАК СТРОКА(100)) КАК Комментарий
</QueryText>
