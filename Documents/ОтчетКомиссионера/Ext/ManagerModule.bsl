		ТекстЗапросаВтТаблицаВидыЗапасов(Запрос, ТекстыЗапроса);
	КонецЕсли; 
	
	ПересчитатьТаблицуТоваров(Запрос);
	
	ТекстЗапроса = "
	|ВЫБРАТЬ
	|	ПересчитаннаяТаблицаДокумента.НомерСтроки КАК НомерСтроки,
	|	&Период КАК Период,
	|	&Валюта КАК Валюта,
	|	ПересчитаннаяТаблицаДокумента.ИдентификаторСтроки КАК ИдентификаторСтроки,
	|	ПересчитаннаяТаблицаДокумента.СтавкаНДС КАК СтавкаНДС,
	|	ПересчитаннаяТаблицаДокумента.СуммаБезНДС КАК СуммаБезНДС,
	|	ПересчитаннаяТаблицаДокумента.СуммаНДС КАК СуммаНДС,
	|	ПересчитаннаяТаблицаДокумента.СуммаБезНДСРегл КАК СуммаБезНДСРегл,
	|	ПересчитаннаяТаблицаДокумента.СуммаНДСРегл КАК СуммаНДСРегл,
	|	ПересчитаннаяТаблицаДокумента.СуммаБезНДСРегл КАК БазаНДСРегл,
	|	ПересчитаннаяТаблицаДокумента.СуммаБезНДСУпр КАК БазаНДСУпр,
	|	НЕОПРЕДЕЛЕНО КАК ТипРасчетов,
	|	ПересчитаннаяТаблицаДокумента.СуммаБезНДСУпр КАК СуммаБезНДСУпр,
	|	ПересчитаннаяТаблицаДокумента.СуммаНДСУпр КАК СуммаНДСУпр
	|
	|ИЗ
	|	ТаблицаТоваровПредварительная КАК ПересчитаннаяТаблицаДокумента
	|
	|ОБЪЕДИНИТЬ ВСЕ
	|
	|ВЫБРАТЬ
	|	0 КАК НомерСтроки,
	|	&Период КАК Период,
	|	&Валюта КАК Валюта,
	|	"""" КАК ИдентификаторСтроки,
	|	Документ.СтавкаНДСВознаграждения КАК СтавкаНДС,
	|	Документ.СуммаВознаграждения - Документ.СуммаНДСВознаграждения КАК СуммаБезНДС,
	|	Документ.СуммаНДСВознаграждения КАК СуммаНДС,
	|
	|	ВЫБОР КОГДА &Валюта = &ВалютаРегламентированногоУчета ТОГДА
	|		Документ.СуммаВознаграждения - Документ.СуммаНДСВознаграждения
