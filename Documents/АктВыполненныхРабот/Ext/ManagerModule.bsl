#Область Обработчики_11_4_1

// Регистрирует данные для обработчика обновления УТ 11.4.1
//
Процедура ЗарегистрироватьДанныеКОбработкеДляПереходаНаНовуюВерсию(Параметры) Экспорт
	
	Запрос = Новый Запрос("
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	Услуги.Ссылка КАК Ссылка
	|ИЗ
	|	Документ.АктВыполненныхРабот.Услуги КАК Услуги
	|ГДЕ
	|	Услуги.ЗаказКлиента.Назначение <> Услуги.Назначение
	|	И Услуги.Номенклатура.ТипНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Работа)
	|	И Услуги.Ссылка.АктПоЗаказам
	|	И НЕ Услуги.Ссылка.ПометкаУдаления
	|
	|ОБЪЕДИНИТЬ ВСЕ
	|
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	Услуги.Ссылка КАК Ссылка
	|ИЗ
	|	Документ.АктВыполненныхРабот.Услуги КАК Услуги
	|ГДЕ
	|	Услуги.Ссылка.НаправлениеДеятельности.Назначение <> Услуги.Назначение
	|	И Услуги.Ссылка.НаправлениеДеятельности.УчетЗатрат
	|	И Услуги.Номенклатура.ТипНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Работа)
	|	И НЕ Услуги.Ссылка.АктПоЗаказам
	|	И НЕ Услуги.Ссылка.ПометкаУдаления");
	
	ОбновлениеИнформационнойБазы.ОтметитьКОбработке(Параметры, Запрос.Выполнить().Выгрузить().ВыгрузитьКолонку("Ссылка"));
	
КонецПроцедуры

// Переносит данные о назначении из заказа клиента/направления деятельности в ТЧ Услуги.
//
Процедура ОбработатьДанныеДляПереходаНаНовуюВерсию(Параметры) Экспорт
	
	ПолноеИмяДокумента = "Документ.АктВыполненныхРабот";
	МетаданныеДокумента = Метаданные.Документы.АктВыполненныхРабот;
	
	ДополнительныеПараметрыВыборкиДанных = ОбновлениеИнформационнойБазы.ДополнительныеПараметрыВыборкиДанныхДляОбработки();
	ДополнительныеПараметрыВыборкиДанных.ДополнительныеИсточникиДанных.Вставить("Услуги.Номенклатура");
	ДополнительныеПараметрыВыборкиДанных.ДополнительныеИсточникиДанных.Вставить("Услуги.ЗаказКлиента");
	ДополнительныеПараметрыВыборкиДанных.ДополнительныеИсточникиДанных.Вставить("НаправлениеДеятельности");
	
	Выборка = ОбновлениеИнформационнойБазы.ВыбратьСсылкиДляОбработки(Параметры.Очередь,
		ПолноеИмяДокумента,
		ДополнительныеПараметрыВыборкиДанных);
	
	Пока Выборка.Следующий() Цикл
		
		НачатьТранзакцию();
		
		Попытка
			Блокировка = Новый БлокировкаДанных;
			ЭлементБлокировки = Блокировка.Добавить(ПолноеИмяДокумента);
			ЭлементБлокировки.УстановитьЗначение("Ссылка", Выборка.Ссылка);
			ЭлементБлокировки.Режим = РежимБлокировкиДанных.Исключительный;
			Блокировка.Заблокировать();
		Исключение
			ОтменитьТранзакцию();
			ТекстСообщения = НСтр("ru = 'Не удалось заблокировать документ: %Ссылка% по причине: %Причина%'");
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%Ссылка%", Выборка.Ссылка);
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%Причина%", ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			ЗаписьЖурналаРегистрации(ОбновлениеИнформационнойБазы.СобытиеЖурналаРегистрации(),
									УровеньЖурналаРегистрации.Предупреждение,
									МетаданныеДокумента,
									Выборка.Ссылка,
									ТекстСообщения);
			Продолжить;
		КонецПопытки;
		
		ДокументОбъект = Выборка.Ссылка.ПолучитьОбъект();
		Если ДокументОбъект = Неопределено Тогда
			ОбновлениеИнформационнойБазы.ОтметитьВыполнениеОбработки(Выборка.Ссылка);
			ЗафиксироватьТранзакцию();
			Продолжить;
		КонецЕсли;
		
		Если ЗначениеЗаполнено(ДокументОбъект.НаправлениеДеятельности) Тогда
			Попытка
				Блокировка = Новый БлокировкаДанных;
				ЭлементБлокировки = Блокировка.Добавить("Справочник.НаправленияДеятельности");
				ЭлементБлокировки.УстановитьЗначение("Ссылка", ДокументОбъект.НаправлениеДеятельности);
				ЭлементБлокировки.Режим = РежимБлокировкиДанных.Разделяемый;
				Блокировка.Заблокировать();
			Исключение
				// Пропуск обработки объекта.
				ОтменитьТранзакцию();
				Продолжить;
			КонецПопытки;
		КонецЕсли;
		
		ВсяНоменклатура         = Новый Соответствие();
		Заказы                  = Новый Соответствие();
		
		Для каждого СтрокаТЧ Из ДокументОбъект.Услуги Цикл
			Если ВсяНоменклатура[СтрокаТЧ.Номенклатура] = Неопределено Тогда
				ВсяНоменклатура.Вставить(СтрокаТЧ.Номенклатура, Истина);
			КонецЕсли;
			Если Заказы[СтрокаТЧ.ЗаказКлиента] = Неопределено Тогда
				Заказы.Вставить(СтрокаТЧ.ЗаказКлиента, Истина);
			КонецЕсли;
		КонецЦикла;
		
		Попытка
			Блокировка = Новый БлокировкаДанных;
			
			Для Каждого Номенклатура Из ВсяНоменклатура Цикл
				
				ЭлементБлокировки = Блокировка.Добавить("Справочник.Номенклатура");
				ЭлементБлокировки.УстановитьЗначение("Ссылка", Номенклатура.Ключ);
				ЭлементБлокировки.Режим = РежимБлокировкиДанных.Разделяемый;
				
			КонецЦикла;
			
			Для Каждого Заказ Из Заказы Цикл
				Если ТипЗнч(Заказ.Ключ) = Тип("ДокументСсылка.ЗаказКлиента") Тогда
					ЭлементБлокировки = Блокировка.Добавить("Документ.ЗаказКлиента");
				ИначеЕсли ТипЗнч(Заказ.Ключ) = Тип("ДокументСсылка.ЗаявкаНаВозвратТоваровОтКлиента") Тогда
					ЭлементБлокировки = Блокировка.Добавить("Документ.ЗаявкаНаВозвратТоваровОтКлиента");
				Иначе
					Продолжить;
				КонецЕсли;
				ЭлементБлокировки.УстановитьЗначение("Ссылка", Заказ.Ключ);
				ЭлементБлокировки.Режим = РежимБлокировкиДанных.Разделяемый;
				
			КонецЦикла;
		
			Блокировка.Заблокировать();
		Исключение
			// Пропуск обработки объекта.
			ОтменитьТранзакцию();
			Продолжить;
		КонецПопытки;
		
		Запрос = Новый Запрос();
		Запрос.Текст =
			"ВЫБРАТЬ
			|	Услуги.НомерСтроки КАК НомерСтроки,
			|	Услуги.ЗаказКлиента.Назначение КАК Назначение
			|ИЗ
			|	Документ.АктВыполненныхРабот.Услуги КАК Услуги
			|ГДЕ
			|	Услуги.ЗаказКлиента.Назначение <> Услуги.Назначение
			|	И Услуги.Номенклатура.ТипНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Работа)
			|	И Услуги.Ссылка.АктПоЗаказам
			|	И Услуги.Ссылка = &Ссылка
			|
			|ОБЪЕДИНИТЬ ВСЕ
			|
			|ВЫБРАТЬ
			|	Услуги.НомерСтроки,
			|	Услуги.Ссылка.НаправлениеДеятельности.Назначение
			|ИЗ
			|	Документ.АктВыполненныхРабот.Услуги КАК Услуги
			|ГДЕ
			|	Услуги.Ссылка.НаправлениеДеятельности.Назначение <> Услуги.Назначение
			|	И Услуги.Ссылка.НаправлениеДеятельности.УчетЗатрат
			|	И Услуги.Номенклатура.ТипНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Работа)
			|	И НЕ Услуги.Ссылка.АктПоЗаказам
			|	И Услуги.Ссылка = &Ссылка";
		
		Запрос.УстановитьПараметр("Ссылка", Выборка.Ссылка);
		ВыборкаРаботы = Запрос.Выполнить().Выбрать();
		
		ДокументОбработан = Ложь;
		Пока ВыборкаРаботы.Следующий() Цикл
			СтрокаАкта = ДокументОбъект.Услуги.Получить(ВыборкаРаботы.НомерСтроки-1);
			СтрокаАкта.Назначение = ВыборкаРаботы.Назначение;
			ДокументОбработан = Истина;
		КонецЦикла;
		
		Попытка
			Если ДокументОбработан Тогда
				ОбновлениеИнформационнойБазы.ЗаписатьОбъект(ДокументОбъект, , , РежимЗаписиДокумента.Запись);
			Иначе
				ОбновлениеИнформационнойБазы.ОтметитьВыполнениеОбработки(Выборка.Ссылка); // в документе заполнять назначение не надо, отмечаем как отработанный
			КонецЕсли;
			ЗафиксироватьТранзакцию();
		Исключение
			ОтменитьТранзакцию();
			ТекстСообщения = НСтр("ru = 'Не удалось обработать документ: %Ссылка% по причине: %Причина%'");
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%Ссылка%", Выборка.Ссылка);
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%Причина%", ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			ЗаписьЖурналаРегистрации(ОбновлениеИнформационнойБазы.СобытиеЖурналаРегистрации(),
				УровеньЖурналаРегистрации.Предупреждение,
				МетаданныеДокумента,
				Выборка.Ссылка,
				ТекстСообщения);
			ВызватьИсключение;
		КонецПопытки;
		
	КонецЦикла;
	Параметры.ОбработкаЗавершена = Не ОбновлениеИнформационнойБазы.ЕстьДанныеДляОбработки(Параметры.Очередь, ПолноеИмяДокумента);
	
КонецПроцедуры

