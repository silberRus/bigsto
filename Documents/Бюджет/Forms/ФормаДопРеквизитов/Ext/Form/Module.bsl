	Дата 					= Параметры.Дата;
	Номер 					= Параметры.Номер;
	ЭтоСистемныйДокумент	= Параметры.ЭтоСистемныйДокумент;
	
КонецПроцедуры

&НаКлиенте
Процедура Изменить(Команда)
	
	Закрыть(Новый Структура("Номер, Дата, ЭтоСистемныйДокумент", Номер, Дата, ЭтоСистемныйДокумент));
	
КонецПроцедуры
